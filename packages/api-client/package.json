{
  "name": "@beyondessential/tupaia-api-client",
  "version": "1.0.1",
  "description": "API client for connecting to Tupaia APIs",
  "main": "dist/index.js",
  "types": "./dist/**/**.d.ts",
  "license": "SEE LICENSE IN LICENSE",
  "scripts": {
    "build": "babel src --out-dir dist --ignore \"src/__tests__/**\" --extensions .ts --config-file \"../../.babelrc-ts.js\"",
    "lint": "tsc; eslint . --ext .ts",
    "lint:fix": "yarn lint --fix",
    "build:typedefs": "tsc",
    "test": "jest --runInBand",
    "test:coverage": "yarn test --coverage",
    "prepublish": "yarn lint; yarn build:typedefs; yarn build"
  },
  "author": "Beyond Essential Systems <admin@tupaia.org> (https://beyondessential.com.au)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beyondessential/tupaia.git",
    "directory": "packages/api-client"
  },
  "dependencies": {
    "node-fetch": "^1.7.3",
    "qs": "^6.10.1"
  }
}
